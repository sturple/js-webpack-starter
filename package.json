{
  "name": "js-webpack-starter",
  "version": "1.0.0",
  "author": "Shawn Turple",
  "description": "Webpack starter kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/sturple/js-webpack-starter.git"
  },
  "bugs": {
    "url": "https://github.com/sturple/js-webpack-starter/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "scripts": {
    "build": "webpack --env.development --progress --config webpack.js  --colors",
    "build:production": "webpack --progress --config webpack.js  --colors",
    "start": "webpack-dev-server --open --env.development --config webpack.js"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "browserslist": [
    "> 1.5%",
    "ie >= 11",
    "edge >= 17"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": "3.0.0",
          "targets": {
            "esmodules": true,
            "ie": "11"
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-classes": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^3.0.2",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-dev-server": "^3.9.0",
    "webpack-cli": "^3.3.9",
    "webpack-merge": "^4.2.2",
    "write-file-webpack-plugin": "^4.5.1",
    "copy-globs-webpack-plugin": "^0.3.0",
    "resolve-url-loader": "^3.1.0"
  },
  "dependencies": {}
}
